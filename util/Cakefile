/* Old RLE Programs (Utah format Edition 2) */

orle-fb:	orle-fb.o LIBFB LIBORLE
	CC LDFLAGS -o orle-fb orle-fb.o LIBFB LIBORLE LIBES

fb-orle:	fb-orle.o linear-cmap.o cmap-crunch.o LIBFB LIBORLE
	CC LDFLAGS -o fb-orle fb-orle.o linear-cmap.o cmap-crunch.o \
		LIBFB LIBORLE LIBES

