		pl-sgi pldebug plgetframe plrot \
		dunncolor dunnsnap cat-fb \
		buffer files-tape \
		plcolor plline2 pl-pl \
		fbhelp

#define	SRCSUFF	.c
#define MANSECTION	1

#include "../Cakefile.defs"
#include "../Cakefile.prog"

/* Explicit composition of each product */

dunncolor:	dunncolor.o dunncomm.o
	CC LDFLAGS -o dunncolor dunncolor.o dunncomm.o LIBES

dunnsnap:	dunnsnap.o dunncomm.o LIBFB
