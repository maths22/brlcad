#define	CONFIGDEFS	MGED_CONFIG

#define SRCDIR	mged
#define PRODUCTS	mged
#define	SRCSUFF	.c
#define MANSECTION	1

#define	SHFILES	newvers.sh newvers5.sh newversVLD.sh

#define FILES	ged dodraw dozoom titles \
			usepen dir columns path mover tedit \
			cmd chgview chgmodel chgtree \
			rtif anal typein \
			buttons clip menu adc edsol edarb \
			ars proc_reg memalloc \
			spline mater plot overlay inside \
			utility1 utility2 track arbs \
			facedef arb ell half tgc torus \
			attach dm-tek dm-plot dm-ps \
			dm-tek4109 MGED_OBJS

#include "../Cakefile.defs"
#include "../Cakefile.prog"

mged:	OBJ
	-rm -f vers.c
	sh ../SRCDIR/newvers.sh
	CC CFLAGS -c vers.c
	CC -o mged LDFLAGS OBJ MGED_LIBES \
		LIBRT RT_LIBES LIBMALLOC LIBES vers.o

#include "../Cakefile.rules"
